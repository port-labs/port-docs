    "docusaurus-theme-openapi-docs": "^4.6.0",
    "pluralize": "^8.0.0",
    "prettier": "^3.8.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-tooltip": "^5.30.0"
